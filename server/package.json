{
  "name": "dashboard-backend",
  "version": "1.0.0",
  "description": "Backend server for Dashboard application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/middleware --coverage",
    "test:integration": "jest tests/integration --coverage",
    "test:performance": "jest tests/performance --coverage",
    "test:auth": "jest tests/integration/auth.test.js",
    "test:cache": "jest tests/integration/cache.test.js",
    "test:rate": "jest tests/performance/rateLimiter.test.js",
    "test:load": "artillery run tests/load/config.yml",
    "test:smoke": "artillery run tests/load/smoke.yml",
    "test:stress": "artillery run tests/load/stress.yml",
    "test:load:report": "artillery run tests/load/config.yml --output load-results.json && artillery report load-results.json"
  },
  "dependencies": {
    "@sentry/node": "^10.29.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compromise": "^14.14.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^17.0.1",
    "double-metaphone": "^2.0.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "natural": "^8.1.0",
    "pg": "^8.11.3",
    "redis": "^5.10.0",
    "string-similarity": "^4.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "artillery": "^2.0.27",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "supertest": "^7.1.4"
  }
}
